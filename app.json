{
  "expo": {
    "name": "MysticTxt",
    "slug": "mystictxt",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "mystictxt",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0A0A1A"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.mystictxt"
    },
    "android": {
      "package": "com.mystictxt",
      "adaptiveIcon": {
        "backgroundColor": "#0A0A1A",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://replit.com/"
        }
      ],
      "expo-font",
      "expo-web-browser",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "EXPO_PUBLIC_DOMAIN": "mystic-text-portals.replit.app",
      "router": {
        "origin": "https://replit.com/"
      },
      "eas": {
        "projectId": "44075026-f043-4dac-98fd-adc3ac4cc86f"
      }
    }
  }
}
